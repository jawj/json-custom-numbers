{
  "name": "json-custom-numbers",
  "version": "1.0.2",
  "description": "JSON parser enabling custom number parsing",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "main": "/dist/index.js",
  "scripts": {
    "test": "node test.mjs",
    "buildEsm": "esbuild src/parse.mjs --bundle --platform=neutral --target=es2015 --minify --format=esm --outfile=dist/index.mjs",
    "buildCjs": "esbuild src/parse.mjs --bundle --platform=neutral --target=es2015 --minify --format=cjs --outfile=dist/index.js",
    "build": "npm run test && npm run buildEsm && npm run buildCjs"
  },
  "author": "George MacKerron",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.17.19"
  },
  "keywords": ["json", "bigint", "parse", "custom", "crockford"]
}