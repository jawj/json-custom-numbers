{
  "name": "json-custom-numbers",
  "version": "2.0.12",
  "description": "JSON parser and stringifier for custom numbers (e.g. BigInt)",
  "files": [
    "./dist",
    "index.d.ts",
    "index.js",
    "index.mjs",
    "./README.md"
  ],
  "main": "./dist/parse.js",
  "scripts": {
    "test": "node --expose-gc test/test.mjs",
    "testPerf": "node --expose-gc test/test.mjs --perf-only",
    "testConf": "node --expose-gc test/test.mjs --conf-only",
    "buildCjs": "esbuild src/*.mjs --platform=neutral --target=es6 --minify --line-limit=80 --format=cjs --outdir=dist",
    "wrapEsm": "npx gen-esm-wrapper dist/index.js dist/index.mjs",
    "build": "npm run testConf && npm run buildCjs && npm run wrapEsm"
  },
  "author": "George MacKerron",
  "license": "Public domain",
  "repository": {
    "type": "git",
    "url": "https://github.com/jawj/json-custom-numbers"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "esbuild": "^0.18.11",
    "gen-esm-wrapper": "^1.1.3",
    "json-bigint": "^1.0.0",
    "lossless-json": "^2.0.10"
  },
  "keywords": [
    "json",
    "parse",
    "stringify",
    "numbers",
    "bigint",
    "custom",
    "lossless",
    "performance",
    "conformance",
    "crockford"
  ]
}
